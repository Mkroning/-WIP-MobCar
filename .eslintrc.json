{
    "env": {
        "es2021": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": "error",
      "class-methods-use-this":"off",
      "import/extensions": [
      "error",
      "ignorePackages",
      {
        "ts": "never"
      }
    ]
    }
}
